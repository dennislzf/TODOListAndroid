<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taskstodo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.taskstodo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.taskstodo.CreateNewToDoActivity"
            android:label="@string/title_activity_create_new_to_do" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testapp.MainActivity" />
        </activity>
        <activity
            android:name="com.example.taskstodo.EditToDoActivity"
            android:label="@string/title_activity_edit_to_do" >
        </activity>
    </application>

</manifest>